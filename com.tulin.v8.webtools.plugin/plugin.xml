            name="%plugin.launcher.javascript"
            public="true">
         <fileExtension
               default="false"
               extension="js"/>
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="com.tulin.v8.webtools.jseditor.launch.JavaScriptLaunchConfigurationType"
