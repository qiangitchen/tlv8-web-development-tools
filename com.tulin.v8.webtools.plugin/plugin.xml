   <!--====================================================-->
   <!-- JavaScript Launcher -->
   <!--====================================================-->
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="com.tulin.v8.webtools.jseditor.launch.JavaScriptLaunchConfigurationDelegate"
            id="com.tulin.v8.webtools.jseditor.launch.JavaScriptLaunchConfigurationType"
