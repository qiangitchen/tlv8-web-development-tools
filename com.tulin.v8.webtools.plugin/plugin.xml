<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <!--====================================================-->
   <!-- Extention points -->
   <!--====================================================-->
   <extension-point id="customTagConverter" name="customTagConverter" schema="schema/customTagConverter.exsd"/>
   <extension-point id="customTagAttributeAssist" name="customTagAttributeAssist" schema="schema/customTagAttributeAssist.exsd"/>
   <extension-point id="paletteItem" name="paletteItem" schema="schema/paletteItem.exsd"/>
   <extension-point id="noValidationNatures" name="noValidationNatures" schema="schema/noValidationNatures.exsd"/>
   <extension-point id="fileAssistProcessor" name="fileAssistProcessor" schema="schema/fileAssistProcessor.exsd"/>
   <extension-point id="hyperlinkProvider" name="hyperlinkProvider" schema="schema/hyperlinkProvider.exsd"/>
   <extension-point id="tldLocator" name="tldLocator" schema="schema/tldLocator.exsd"/>
   <extension-point id="customTagValidator" name="customTagValidator" schema="schema/customTagValidator.exsd"/>
   <extension-point id="preferenceContributer" name="preferenceContributer" schema="schema/preferenceContributer.exsd"/>
   <extension-point id="pagefilter" name="pagefilter" schema="schema/pagefilter.exsd"/>
   <extension-point id="javaScriptCompleter" name="javaScriptCompleter" schema="schema/javaScriptCompleter.exsd"/>
   <extension-point id="jspelAssistProcessor" name="jspelAssistProcessor" schema="schema/jspelAssistProcessor.exsd"/>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="%category.label.source"
            id="tulin.commands.category">
      </category>
      <command
      		categoryId="tulin.commands.category"
            id="tulin.command.comment"
            name="%menu.label.comment">
      </command>
      <command
      		categoryId="tulin.commands.category"
            id="tulin.command.MultilineComment"
            name="%menu.label.mcomment">
      </command>
      <command
      		categoryId="tulin.commands.category"
            id="tulin.command.UnMultilineComment"
            name="%menu.label.umcomment">
      </command>
      <command
      		categoryId="tulin.commands.category"
            id="tulin.command.format"
            name="%menu.label.fortmat">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="tulin.command.comment"
            class="com.tulin.v8.webtools.handlers.ToggleCommentHandler">
      </handler>
      <handler
            commandId="tulin.command.MultilineComment"
            class="com.tulin.v8.webtools.handlers.MultilineCommentHandler">
      </handler>
      <handler
            commandId="tulin.command.UnMultilineComment"
            class="com.tulin.v8.webtools.handlers.UnMultilineCommentHandler">
      </handler>
      <handler
            commandId="tulin.command.format"
            class="com.tulin.v8.webtools.handlers.FormaterHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="tulin.command.comment"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+/"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            commandId="tulin.command.MultilineComment"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+M2+/"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            commandId="tulin.command.UnMultilineComment"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+M2+\"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            commandId="tulin.command.format"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+M2+F"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
   </extension>
   
   <!--====================================================-->
   <!-- Editors -->
   <!--====================================================-->
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.tulin.v8.webtools.htmleditor.editors.HTMLEditor"
            contributorClass="com.tulin.v8.webtools.htmleditor.editors.HTMLEditorContributor"
            extensions="html,xhtml"
            icon="icons/html_file.gif"
            id="com.tulin.v8.webtools.editors.HTMLEditor"
            name="%plugin.editor.htmleditor"/>
      <editor
            class="com.tulin.v8.webtools.csseditor.editors.CSSEditor"
            contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
            default="true"
            extensions="css"
            icon="icons/css.gif"
            id="com.tulin.v8.webtools.csseditor.editors.CSSEditor"
            name="%plugin.editor.csseditor"/>
      <editor
            class="com.tulin.v8.webtools.jspeditor.editors.JSPEditor"
            contributorClass="com.tulin.v8.webtools.jspeditor.editors.JSPEditorContributor"
            default="true"
            extensions="jsp,jspf"
            icon="icons/jsp_file.gif"
            id="com.tulin.v8.webtools.jspeditor.editors.JSPEditor"
            name="%plugin.editor.jspeditor"/>
      <!--
      <editor
            class="com.tulin.v8.webtools.xmleditor.editors.XMLEditor"
            contributorClass="com.tulin.v8.webtools.xmleditor.editors.XMLEditorContributor"
            default="true"
            extensions="xml,tld,xsl"
            icon="icons/xml.gif"
            id="com.tulin.v8.webtools.xmleditor.editors.XMLEditor"
            name="%plugin.editor.xmleditor">
      </editor>
      <editor
            class="com.tulin.v8.webtools.dtdeditor.editors.DTDEditor"
            contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
            default="true"
            extensions="dtd"
            icon="icons/dtd.gif"
            id="com.tulin.v8.webtools.dtdeditor.editors.DTDEditor"
            name="%plugin.editor.dtdeditor"/>
      -->
      <editor
            class="com.tulin.v8.webtools.jseditor.editors.JavaScriptEditor"
            contributorClass="com.tulin.v8.webtools.jseditor.editors.JavaScriptEditorContributor"
            default="true"
            extensions="js"
            icon="icons/js.gif"
            id="com.tulin.v8.webtools.jseditor.editors.JavaScriptEditor"
            name="%plugin.editor.jseditor"/>
      <!--
      <editor
            class="com.tulin.v8.webtools.editors.WebBrowser"
            extensions="url"
            icon="icons/web.gif"
            id="com.tulin.v8.webtools.editors.WebBrowser"
            name="%plugin.editor.webbrowser"/>
      -->
   </extension>

   <extension
         point="com.tulin.v8.webtools.fileAssistProcessor">
      <processor class="com.tulin.v8.webtools.htmleditor.assist.FileAssistProcessor"/>
   </extension>
   <extension
         point="com.tulin.v8.webtools.customTagAttributeAssist">
      <customTagAttributeAssist
            class="com.tulin.v8.webtools.jspeditor.editors.JSTLAttributeAssist">
      </customTagAttributeAssist>
   </extension>

   <!--====================================================-->
   <!-- Wizards -->
   <!--====================================================-->
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="%plugin.preference.category"
            id="com.tulin.v8.webtools"/>
      <wizard
            class="com.tulin.v8.webtools.htmleditor.wizards.HTMLNewWizard"
            icon="icons/html_file.gif"
            category="com.tulin.v8.webtools"
            name="%plugin.wizard.html"
            id="com.tulin.v8.webtools.wizards.HTMLNewWizard"/>
      <!--
      <wizard
            icon="icons/web.gif"
            class="com.tulin.v8.webtools.htmleditor.wizards.URLNewWizard"
            category="com.tulin.v8.webtools"
            name="%plugin.wizard.url"
            id="com.tulin.v8.webtools.wizards.URLNewWizard"/>
      -->
      <wizard
            class="com.tulin.v8.webtools.htmleditor.wizards.JSPNewWizard"
            icon="icons/jsp_file.gif"
            category="com.tulin.v8.webtools"
            name="%plugin.wizard.jsp"
            id="com.tulin.v8.webtools.wizards.JSPNewWizard"/>
      <wizard
            class="com.tulin.v8.webtools.htmleditor.wizards.XMLNewWizard"
            icon="icons/xml.gif"
            category="com.tulin.v8.webtools"
            name="%plugin.wizard.xml"
            id="com.tulin.v8.webtools.wizards.XMLNewWizard"/>
      <wizard
            category="com.tulin.v8.webtools"
            class="com.tulin.v8.webtools.jseditor.wizards.AddJavaScriptLibraryWizard"
            icon="icons/js.gif"
            id="com.tulin.v8.webtools.jseditor.wizards.AddJavaScriptLibraryWizard"
            name="%plugin.wizard.jslibrary">
      </wizard>
      
      <wizard
            canFinishEarly="false"
            category="com.tulin.v8.webtools"
            class="com.tulin.v8.webtools.wizards.NewWebProjectWizard"
            finalPerspective="com.tulin.v8.ide.perspectives.RelEngPerspective"
            hasPages="true"
            icon="icons/web.gif"
            id="com.tulin.v8.webtools.wizards.NewWebProjectWizard"
            name="%np.title"
            project="true">
        <description>
            %np.desc
        </description>
       </wizard>
   </extension>
   
