<?xml version="1.0" encoding="UTF-8"?>
<plugin>
	<!--====================================================-->
   <!-- Extention points -->
   <!--====================================================-->
   <extension-point id="customTagConverter" name="customTagConverter" schema="schema/customTagConverter.exsd"/>
   <extension-point id="customTagAttributeAssist" name="customTagAttributeAssist" schema="schema/customTagAttributeAssist.exsd"/>
   <extension-point id="paletteItem" name="paletteItem" schema="schema/paletteItem.exsd"/>
   <extension-point id="noValidationNatures" name="noValidationNatures" schema="schema/noValidationNatures.exsd"/>
   <extension-point id="fileAssistProcessor" name="fileAssistProcessor" schema="schema/fileAssistProcessor.exsd"/>
   <extension-point id="hyperlinkProvider" name="hyperlinkProvider" schema="schema/hyperlinkProvider.exsd"/>
   <extension-point id="tldLocator" name="tldLocator" schema="schema/tldLocator.exsd"/>
   <extension-point id="customTagValidator" name="customTagValidator" schema="schema/customTagValidator.exsd"/>
   <extension-point id="preferenceContributer" name="preferenceContributer" schema="schema/preferenceContributer.exsd"/>
   <extension-point id="pagefilter" name="pagefilter" schema="schema/pagefilter.exsd"/>
   <extension-point id="javaScriptCompleter" name="javaScriptCompleter" schema="schema/javaScriptCompleter.exsd"/>
   <extension-point id="jspelAssistProcessor" name="jspelAssistProcessor" schema="schema/jspelAssistProcessor.exsd"/>
   <extension-point id="contentAssistProcessors" name="contentAssistProcessors" schema="schema/contentAssistProcessors.exsd"/>
   <extension-point id="hoverProviders" name="hoverProviders" schema="schema/hoverProviders.exsd"/>
   
	<!-- 初始配置 -->
	<extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="com.tulin.v8.webtools.PreferenceInitializer"/>
   	</extension>
   
	<!-- HTML File Editor-->
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.text"
            file-extensions="htm,html,xhtml"
            id="com.tulin.v8.webtools.html"
            name="HTML">
      </content-type>
   </extension>
   <!--
   <extension
         point="org.eclipse.ui.editors">
      <editorContentTypeBinding
            contentTypeId="com.tulin.v8.webtools.html"
            editorId="org.eclipse.ui.genericeditor.GenericEditor">
      </editorContentTypeBinding>
   </extension>
   <extension point="org.eclipse.ui.genericeditor.icons">
    <icon contentType="com.tulin.v8.webtools.html" icon="icons/html.png"/>
   </extension>
   -->
   
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.tulin.v8.webtools.html.editors.HTMLEditor"
            contributorClass="com.tulin.v8.webtools.html.editors.HTMLEditorContributor"
            extensions="html,xhtml"
            icon="icons/html_file.gif"
            id="com.tulin.v8.webtools.editors.HTMLEditor"
            name="%plugin.editor.htmleditor"/>
      <editor
            class="com.tulin.v8.webtools.css.editors.CSSEditor"
            contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
            default="true"
            extensions="css"
            icon="icons/css.gif"
            id="com.tulin.v8.webtools.csseditor.editors.CSSEditor"
            name="%plugin.editor.csseditor"/>
      <editor
            class="com.tulin.v8.webtools.js.editors.JavaScriptEditor"
            contributorClass="com.tulin.v8.webtools.js.editors.JavaScriptEditorContributor"
            default="true"
            extensions="js"
            icon="icons/js.gif"
            id="com.tulin.v8.webtools.jseditor.editors.JavaScriptEditor"
            name="%plugin.editor.jseditor"/>
            
      <editor
            class="com.tulin.v8.webtools.jsp.editors.JSPEditor"
            contributorClass="com.tulin.v8.webtools.jsp.editors.JSPEditorContributor"
            default="true"
            extensions="jsp,jspf"
            icon="icons/jsp_file.gif"
            id="com.tulin.v8.webtools.jspeditor.editors.JSPEditor"
            name="%plugin.editor.jspeditor"/>
            
      <editor
            class="com.tulin.v8.webtools.xml.editors.XMLEditor"
            contributorClass="com.tulin.v8.webtools.xml.editors.XMLEditorContributor"
            default="true"
            extensions="xml,tld,xsl"
            icon="icons/xml.gif"
            id="com.tulin.v8.webtools.xmleditor.editors.XMLEditor"
            name="%plugin.editor.xmleditor">
      </editor>
   </extension>
   
   <extension
         point="com.tulin.v8.webtools.fileAssistProcessor">
      <processor class="com.tulin.v8.webtools.assist.FileAssistProcessor"/>
   </extension>
   <extension
         point="com.tulin.v8.webtools.customTagAttributeAssist">
      <customTagAttributeAssist
            class="com.tulin.v8.webtools.jsp.editors.JSTLAttributeAssist">
      </customTagAttributeAssist>
   </extension>
   
   <!--====================================================-->
   <!-- Preferences -->
   <!--====================================================-->
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.tulin.v8.webtools.preferencePages.PreferencePage"
            name="%plugin.preference.category"
            category="com.tulin.v8.ide.preferences.StudioPreferencePage"
            id="com.tulin.v8.webtools.category"/>
      <page
            class="com.tulin.v8.webtools.preferencePages.CustomAssistPreferencePage"
            category="com.tulin.v8.webtools.AssistPreferencePage"
            name="%plugin.preference.customassist"
            id="com.tulin.v8.webtools.CustomAssistPreferencePage"/>
      <page
            class="com.tulin.v8.webtools.preferencePages.DTDPreferencePage"
            category="com.tulin.v8.webtools.XMLPreferencePage"
            name="%plugin.preference.dtd"
            id="com.tulin.v8.webtools.DTDPreferencePage"/>
      <page
            class="com.tulin.v8.webtools.preferencePages.TLDPreferencePage"
            category="com.tulin.v8.webtools.JSPEditorPreferencePage"
            name="%plugin.preference.tld"
            id="com.tulin.v8.webtools.TLDPreferencePage"/>
      <page
            class="com.tulin.v8.webtools.preferencePages.AssistPreferencePage"
            category="com.tulin.v8.webtools.category"
            name="%plugin.preference.codeassist"
            id="com.tulin.v8.webtools.AssistPreferencePage"/>
      <page
            class="com.tulin.v8.webtools.preferencePages.CSSEditorPreferencePage"
            category="com.tulin.v8.webtools.category"
            name="%plugin.preference.csseditor"
            id="com.tulin.v8.webtools.CSSEditorPreferencePage"/>
      <page
            class="com.tulin.v8.webtools.preferencePages.JSPEditorPreferencePage"
            category="com.tulin.v8.webtools.category"
            name="%plugin.preference.jspeditor"
            id="com.tulin.v8.webtools.JSPEditorPreferencePage"/>
      <page
            class="com.tulin.v8.webtools.preferencePages.JavaScriptEditorPreferencePage"
            category="com.tulin.v8.webtools.category"
            name="%plugin.preference.jseditor"
            id="com.tulin.v8.webtools.JavaScriptEditorPreferencePage"/>
      <page
            class="com.tulin.v8.webtools.preferencePages.XMLPreferencePage"
            category="com.tulin.v8.webtools.category"
            name="XML"
            id="com.tulin.v8.webtools.XMLPreferencePage"/>
      <page
            class="com.tulin.v8.webtools.preferencePages.HTMLTaskTagPreferencePage"
            category="com.tulin.v8.webtools.XMLPreferencePage"
            name="%plugin.preference.tasktag"
            id="com.tulin.v8.webtools.tasktag.HTMLTaskTagPreferencePage"/>
      <page
            class="com.tulin.v8.webtools.preferencePages.HTMLTemplatePreferencePage"
            category="com.tulin.v8.webtools.AssistPreferencePage"
            name="%plugin.preference.template"
            id="com.tulin.v8.webtools.template.HTMLTemplatePreferencePage"/>
      <page
            class="com.tulin.v8.webtools.preferencePages.FormatterPreferencePage"
            category="com.tulin.v8.webtools.category"
            name="%plugin.preference.formatter"
            id="com.tulin.v8.webtools.FormatterPreferencePage"/>
   </extension>
   
   <extension
         point="org.eclipse.ui.commands">
      <category
            name="%category.label.source"
            id="tulin.commands.category">
      </category>
      <command
      		categoryId="tulin.commands.category"
            id="tulin.command.comment"
            name="%menu.label.comment">
      </command>
      <command
      		categoryId="tulin.commands.category"
            id="tulin.command.MultilineComment"
            name="%menu.label.mcomment">
      </command>
      <command
      		categoryId="tulin.commands.category"
            id="tulin.command.UnMultilineComment"
            name="%menu.label.umcomment">
      </command>
      <command
      		categoryId="tulin.commands.category"
            id="tulin.command.format"
            name="%menu.label.fortmat">
      </command>
   </extension>
   
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="tulin.command.comment"
            class="com.tulin.v8.webtools.handlers.ToggleCommentHandler">
      </handler>
      <handler
            commandId="tulin.command.MultilineComment"
            class="com.tulin.v8.webtools.handlers.MultilineCommentHandler">
      </handler>
      <handler
            commandId="tulin.command.UnMultilineComment"
            class="com.tulin.v8.webtools.handlers.UnMultilineCommentHandler">
      </handler>
      <handler
            commandId="tulin.command.format"
            class="com.tulin.v8.webtools.handlers.FormaterHandler">
      </handler>
   </extension>
   
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="tulin.command.comment"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+/"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            commandId="tulin.command.MultilineComment"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+M2+/"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            commandId="tulin.command.UnMultilineComment"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+M2+\"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            commandId="tulin.command.format"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+M2+F"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
   </extension>
   
   <!--====================================================-->
   <!-- Template -->
   <!--====================================================-->
   <extension
         point="org.eclipse.ui.editors.templates">
      <contextType
            class="com.tulin.v8.webtools.template.HTMLContextType"
            name="HTML/JSP/XML"
            id="com.tulin.v8.webtools.templateContextType.html"/>
      <contextType
            class="com.tulin.v8.webtools.template.JavaScriptContextType"
            name="JavaScript"
            id="com.tulin.v8.webtools.templateContextType.javascript"/>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.catch"
            id="com.tulin.v8.webtools.template.js.catch"
            name="catch">
         <pattern>
catch (e) {
	${cursor}// ${todo}: handle exception
}
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.do"
            id="com.tulin.v8.webtools.template.js.do"
            name="do">
         <pattern>
do {
	${line_selection}${cursor}
} while (${condition});
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.else"
            id="com.tulin.v8.webtools.template.js.else"
            name="else">
         <pattern>
else {
	${cursor}
}
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.elseif"
            id="com.tulin.v8.webtools.template.js.elseif"
            name="elseif">
         <pattern>
else if (${condition}) {
	${cursor}
}
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.for"
            id="com.tulin.v8.webtools.template.js.for"
            name="for">
         <pattern>
for (var ${index} = 0; ${index} &lt; ${array}.length; ${index}++) {
	${line_selection}${cursor}
}
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.forwithvar"
            id="com.tulin.v8.webtools.template.js.forwithvar"
            name="for">
         <pattern>
for (var ${index} = 0; ${index} &lt; ${array}.length; ${index}++) {
	var ${array_element} = ${array}[${index}];
	${cursor}
}
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.forin"
            id="com.tulin.v8.webtools.template.js.forin"
            name="forin">
         <pattern>
for (var ${iterable_element} in ${iterable}) {
	${cursor}
}
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.function"
            id="com.tulin.v8.webtools.template.js.function"
            name="function">
         <pattern>
function ${name}(${args}) {
    ${cursor}
}
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.anonymousFunction"
            id="com.tulin.v8.webtools.template.js.anonymousFunction"
            name="function">
         <pattern>
function (${args}) {
    ${cursor}
}
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.if"
            id="com.tulin.v8.webtools.template.js.if"
            name="if">
         <pattern>
if (${condition}) {
	${line_selection}${cursor}
}
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.ifelse"
            id="com.tulin.v8.webtools.template.js.ifelse"
            name="ifelse">
         <pattern>
if (${condition}) {
	${cursor}
} else {

}
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.lazy"
            id="com.tulin.v8.webtools.template.js.lazy"
            name="lazy">
         <pattern>
if (${name:var} == null) {
	${name} = new ${type}(${arguments});
	${cursor}
}

return ${name};
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.new"
            id="com.tulin.v8.webtools.template.js.new"
            name="new">
         <pattern>
new ${type}(${arguments});
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.newwithvar"
            id="com.tulin.v8.webtools.template.js.newwithvar"
            name="new">
         <pattern>
var ${name} = new ${type}(${arguments});
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.require"
            id="com.tulin.v8.webtools.template.js.require"
            name="require">
         <pattern>
require('${value}')
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.requirewithvar"
            id="com.tulin.v8.webtools.template.js.requirewithvar"
            name="require">
         <pattern>
var ${name} = require('${value}');
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.switch"
            id="com.tulin.v8.webtools.template.js.switch"
            name="switch">
         <pattern>
switch (${key}) {
	case ${value}:
		${cursor}
		break;

	default:
		break;
}
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.try"
            id="com.tulin.v8.webtools.template.js.try"
            name="try">
         <pattern>
try {
	${line_selection}${cursor}
} catch (${e}) {
	// ${todo}: handle exception
}
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.var"
            id="com.tulin.v8.webtools.template.js.var"
            name="var">
         <pattern>
var ${name};
         </pattern>
      </template>
      <template
            autoinsert="false"
            contextTypeId="com.tulin.v8.webtools.templateContextType.javascript"
            description="%plugin.template.js.while"
            id="com.tulin.v8.webtools.template.js.while"
            name="while">
         <pattern>
while (${condition}) {
	${line_selection}${cursor}
}
         </pattern>
      </template>
   </extension>
   
   <!-- Javascript File Editor-->
    <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.text"
            file-extensions="js"
            id="com.tulin.v8.webtools.js"
            name="Javascript">
      </content-type>
   </extension>
