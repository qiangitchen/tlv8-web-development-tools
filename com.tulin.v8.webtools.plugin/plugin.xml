   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="com.tulin.v8.webtools.htmleditor.HTMLPreferenceInitializer"/>
   </extension>

   <!--====================================================-->
   <!-- Views -->
   <!--====================================================-->
   <extension
         point="org.eclipse.ui.views">
      <category
            name="%plugin.preference.category"
            id="com.tulin.v8.webtools"/>
      <view
            icon="icons/image.gif"
            category="com.tulin.v8.webtools"
            class="com.tulin.v8.webtools.htmleditor.views.ImageView"
            name="%plugin.views.image"
            id="com.tulin.v8.webtools.views.ImageView"/>

      <view
