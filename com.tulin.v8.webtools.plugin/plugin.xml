      <page
            category="com.tulin.v8.webtools.HTMLProjectPropertyPage"
            class="com.tulin.v8.webtools.htmleditor.JavaScriptPropertyPage"
            id="com.tulin.v8.webtools.JavaScriptPropertyPage"
            name="JavaScript">
